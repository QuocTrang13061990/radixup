# Options -Indexes
RewriteEngine on

# Change '/radixup/' if directory is renamed
# RewriteBase /radixup/

# Change the '/radixup/' portion of the condition if directory is renamed 
# RewriteCond %{REQUEST_URI} ^/radixup/admin(.+)$
